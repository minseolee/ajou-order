{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ajou-eats-api.seunglab.dev/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://ajou-eats-api.seunglab.dev/api/:path*/"
    },
    {
      "source": "/ws/:path*",
      "destination": "wss://ajou-eats-api.seunglab.dev/ws/:path*"
    },
    {
      "source": "/ws/:path*/",
      "destination": "wss://ajou-eats-api.seunglab.dev/ws/:path*/"
    }
  ]
}
