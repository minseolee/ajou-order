{
  "rewrites": [
    {
      "source": "/api",
      "destination": "https://ajou-eats-api.seunglab.dev/api"
    },
    {
      "source": "/api*",
      "destination": "https://ajou-eats-api.seunglab.dev/api*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://ajou-eats-api.seunglab.dev/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://ajou-eats-api.seunglab.dev/api/:path*/"
    }
  ]
}
